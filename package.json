  "scripts": {
    "test": "npx jest tests/test.js",
    "start": "cds-serve",
    "watch-incidents": "cds watch --open incidents/webapp/index.html?sap-ui-xx-viewCache=false"
  },
"cds": {
  "requires": {
    "API_BUSINESS_PARTNER": {
      "kind": "odata",
      "model": "srv/external/API_BUSINESS_PARTNER",
      "credentials": {
        "destination": "API_BUSINESS_PARTNER",
        "path": "odata/v2/api-business-partner"
      },
      "[production]": {
        "credentials": {
          "destination": "your-destination-name",
          "path": "odata/v2/api-business-partner"
        }
      }
    },
    "[development]": {
      "auth": {
        "kind": "mocked",
        "users": {
          "incident.support@tester.sap.com": {
            "password": "initial",
            "roles": ["support"]
          },
          "alice": {
            "roles": ["support"]
          },
          "bob": {
            "roles": ["support"]
          }
        }
      }
    },
    "[production]": {
      "db": "hana",
      "auth": "xsuaa",
      "destination": "your-destination-name",
      "path": "odata/v2/api-business-partner"
    },
    "destinations": true,
    "html5-runtime": true,
    "workzone": true
  }
},
  "sapux": [
    "app/incidents",
    "app/incidents"
  ]
}
